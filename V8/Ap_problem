/*For the AP whose general term is 3*n +7 find the nth term*/

#include <iostream>

using namespace std;

int nthTerm(int n){
    int ans = 3*n +7;
    return ans;
}


int main()
{
    int n;
    cout<<"Enter n: ";
    cin>>n;

    cout<<"The "<<n<<" term for the given Airthmetic Progression is: "<<nthTerm(n);
    return 0;
}


/*OUTPUT

Enter n: 3
The 3 term for the given Airthmetic Progression is: 16
*/
