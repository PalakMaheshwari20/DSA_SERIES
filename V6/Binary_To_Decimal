/*Binary TO Decimal CONVERSION*/

#include <iostream>
#include<math.h>
using namespace std;

int main()
{
    int n;
    int ans = 0,i=0;
    cin>>n;
    
    while(n!=0){
        int digit = n%10;
        if(digit==1)
        ans += pow(2,i);
        
        n/=10;
        i++;
    }
    
    cout<<ans;
    return 0;
}


/*OUTPUT

100000000000
256

*/
